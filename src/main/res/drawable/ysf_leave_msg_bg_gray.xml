<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/ysf_white" />
            <corners
                android:topLeftRadius="2.5dip"
                android:topRightRadius="2.5dip"
                android:bottomLeftRadius="2.5dip"
                android:bottomRightRadius="2.5dip" />


        </shape>
    </item>
    <item
        android:top="1px"
        android:bottom="1px"
        android:left="1px"
        android:right="1px">
        <shape>
            <gradient
                android:startColor="@color/ysf_white"
                android:endColor="@color/ysf_white" />
            <corners
                android:topLeftRadius="2.5dip"
                android:topRightRadius="2.5dip"
                android:bottomLeftRadius="2.5dip"
                android:bottomRightRadius="2.5dip" />
            <stroke android:color="#E1E3E6"
                android:width="0.5dp"/>
        </shape>
    </item>

</layer-list>